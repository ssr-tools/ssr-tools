{
  "name": "ssr-tools",
  "version": "0.0.0",
  "description": "A set of simple tools for a server-side rendered application development with React",
  "scripts": {
    "prepare": "npx husky install",
    "prepublishOnly": "echo \"Do not publish the root folder.\" && exit 1",
    "test": "jest --config ./jest.config.js",
    "test:e2e": "jest --config ./jest.e2e.config.js --runInBand",
    "tsc": "npx tsc",
    "lint": "eslint ."
  },
  "author": "Grzegorz Zadora",
  "license": "MIT",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/jest": "^29.0.3",
    "@types/jest-environment-puppeteer": "^5.0.2",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "eslint": "^8.23.1",
    "eslint-config-ssr-tools": "file:lib/eslint-config-ssr-tools",
    "husky": "^8.0.1",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "jest-puppeteer": "^6.1.1",
    "lint-staged": "^13.0.3",
    "ts-jest": "^29.0.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "workspaces": [
    "./lib/core",
    "./lib/router",
    "./lib/async-data-store",
    "./lib/static-data-store",
    "./lib/eslint-config-ssr-tools",
    "./lib/css",
    "./examples/with-fastify",
    "./examples/with-spa"
  ],
  "engines": {
    "node": ">=16 <=18",
    "npm": ">=7 <=8"
  }
}
